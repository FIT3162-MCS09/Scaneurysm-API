{
    "production": {
        "aws_region": "ap-southeast-1",
        "project_name": "shap-analysis",
        "runtime": "python3.10",
        "s3_bucket": "zappa-mcs09-deployment",
        "manage_roles": false,
        "role_name": "ecsTaskExecutionRole",
        "role_arn": "arn:aws:iam::522814726499:role/ecsTaskExecutionRole",
